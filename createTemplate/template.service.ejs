import { Injectable } from '@nestjs/common';
import { InjectRepository as injectRepository } from '@nestjs/typeorm';
import { <%= firstUpperCase(name) %> } from './entities/<%= name %>.entity';
import { Repository } from 'typeorm';
import { Response } from '../utils/response';

@Injectable()
/** 匿名类 */
export class <%= firstUpperCase(name) %>Service {
  /** 构造函数 */
  constructor(
    @injectRepository(<%= firstUpperCase(name) %>)
    private <%= name %>Repository: Repository<<%= firstUpperCase(name) %>>,
  ) {}

  /** 创建 */
  async create(content: string) {
    await this.<%= name %>Repository.save({
      content: content,
    });
    return Response.sucess();
  }
}
